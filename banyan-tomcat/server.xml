<?xml version="1.0" encoding="UTF-8"?>
<Server port="8005" shutdown="SHUTDOWN">
	<Service name="Catalina">
		<Connector URIEncoding="UTF-8" acceptCount="100"
			connectionTimeout="20000" disableUploadTimeout="true" enableLookups="false"
			maxHttpHeaderSize="8192" maxSpareThreads="75" maxThreads="150"
			minSpareThreads="25" port="8080" redirectPort="8443" />
		<Connector enableLookups="false" port="8009" protocol="AJP/1.3"
			redirectPort="8443" />
		<Engine defaultHost="localhost" name="Catalina">
			<Host appBase="webapps" autoDeploy="true" name="localhost"
				printToScreen="true" unpackWARs="true" xmlNamespaceAware="false"
				xmlValidation="false">
				<!-- path="" for when I go to NFSN -->
				<Context docBase="species-tapestry-1.2-SNAPSHOT" path=""
					reloadable="true"
					source="org.eclipse.jst.j2ee.server:species-tapestry-1.2-SNAPSHOT" />
				<!-- delete this when I go to NFSN, or maybe it will just "do nothing" -->
				<Context docBase="D:/banyan-images" path="/banyan-images" />
			</Host>
		</Engine>
	</Service>
</Server>