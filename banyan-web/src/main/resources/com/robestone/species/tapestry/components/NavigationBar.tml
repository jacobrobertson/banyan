<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
   "http://www.w3.org/TR/html4/strict.dtd">
<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
<t:form autofocus="focusOnSearch" clientValidation="false">
<t:corners>
	<div class="HeaderDiv">
 	<t:textfield value="search" class="Box" />
 	<t:submit t:id="search" value="Search" class="Button" />
  	<span class="Spacer">|</span>
  	<a href="search.tree/${crunchedIds}" class="Link">#Tree</a>
  	<span class="Spacer">|</span>
  	<t:actionlink t:id="startOver" class="Link">Start Over</t:actionlink>
  	<span class="Spacer">|</span>
  	<t:pagelink page="Examples" class="Link">Examples</t:pagelink>
  	<span class="Spacer">|</span>
  	<t:actionlink t:id="random" class="Link">Random</t:actionlink>
  	<span class="Spacer">|</span>
  	<t:pagelink page="HowToUse" class="Link">FAQ</t:pagelink>
  	<span class="Spacer">|</span>
  	<t:pagelink page="Credits" class="Link">Credits</t:pagelink>
  	<span class="Logo"><img src="icons/banyan-title.png" alt="Banyan"/></span>
  	</div>
</t:corners>
<div class="Quote"><t:if test="showExampleCaptions">${exampleCaption}<t:parameter name="else">"${message:Motto}"</t:parameter></t:if></div>
<t:actionlink t:id="showChildren"></t:actionlink>
<t:actionlink t:id="hideChildren"></t:actionlink>
<t:actionlink t:id="focus"></t:actionlink>
<t:actionlink t:id="close"></t:actionlink>
</t:form>
</t:container>